SELECT 
	FSYR,
	QRTR,
	FSPR,
	CAPR,
	CASE FSPR WHEN '1501' THEN '1502' ELSE FSPR END MODPERD,
	OPSGRP,
	COMP,
	COMP_DESCR,
	SUBSIDIARY,
	PARENT,
	ACCT,
	AZTITL ACCT_DESCR,
	ACCT||' - '||RTRIM(AZTITL) ACCT_AND_DESCR,
	PRIME,
	SUBSTR(ACCT,7,6) PRIMESUB,
	CURR,
	FGRP,
	STMT,
	LVL0,
	LVL1,
	LVL2,
	LVL3,
	GLCC,
	EBITDA,
	DEPARTMENT,
	ELIM_TYPE,
	ELIM_REL,
	ELIM_DFGRP,
	DEP_GRP FUNCAREA,
	ELIM_CO,
	OPEN_LOCAL,
	NET_LOCAL,
	END_LOCAL
FROM 
	RLARP.V_TBLCS C 
WHERE 
	FSPR >= '1601' AND 
	FSPR <= '1606'