SELECT
	DIGLCD, A30, DHINCR, DIPART, COUNT(DILIN#) AS NUMBER_LINES, SUM(DIEXTN) AS SUM_EXTN
FROM
	LGDAT.OIH
	LEFT OUTER JOIN LGDAT.OID ON
		DIINV# = DHINV#
	LEFT OUTER JOIN LGDAT.CODE ON
		A2 = 'EE' AND
		LTRIM(RTRIM(A9)) = DIGLCD
WHERE
	DHARYR = 13 AND
	DHARPR = 4 AND
	DIGLCD = 'FRT'
GROUP BY
	DIGLCD, A30, DHINCR, DIPART
ORDER BY 
	DIGLCD