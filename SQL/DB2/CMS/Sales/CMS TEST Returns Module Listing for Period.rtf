SELECT 
ININV#, INIITM, INPLNT, INRAN#, INRNDR, INRNDI, INCRED, INCRD#, INITM#, 
DCINV#, INRCOD, INPT#, ORIG.DHYYPP, INYYPP, CRED.DHYYPP, INTOTI, INFAMT, 
INDPCD, INCUST, 
RTRIM(INRDS1||' '||INRDS2||' '||INRDS3) AS PROB, 
RTRIM(INREC1||' '||INREC2||' '||INREC3) AS RECNOTES 
FROM 
LGTEST.RANS 
LEFT OUTER JOIN LGTEST.CCRH ON 
DCORD# = INCRD# 
LEFT OUTER JOIN LGTEST.OIH CRED ON 
CRED.DHINV# = DCINV# 
LEFT OUTER JOIN LGTEST.OIH ORIG ON 
ORIG.DHINV# = ININV# 
WHERE 
INYYPP = '1303'
