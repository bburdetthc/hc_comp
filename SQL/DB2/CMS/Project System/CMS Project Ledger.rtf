SELECT
	DIGITS(DKACC#) AS ACCOUNT, 
	DIGITS(DKFSYY)||DIGITS(DKFSPR) AS PERIOD, 
	DKSRCE||DKQUAL AS MODULE, 
	DKKEYN, DKREF#, DKREFD, DKADDD, 
	CHAR(DKTDAT) AS TRANDATE,  
	DKPJNM AS PROJECT, 
	DKFUT4 AS USERID, 
	DKAMT AS AMOUNT
FROM
	LGDAT.GLSBAP
WHERE
	DKTDAT >= '2013-07-01' AND
	DKPJNM = '93-07-0100'