SELECT
	DKACC#, DKAMT, DKKEYN, IGCHQ#, IGGROS, IGDISC, IGVEN#, AVTVH#, IDINV#, IDGROS, IDDISC
FROM
	LGDAT.GLSBAP
	LEFT OUTER JOIN LGDAT.CHQR ON
		IGTXR# = DKKEYN AND
		IGFSYY = DKFSYY AND
		IGFSPP = DKFSPR
	LEFT OUTER JOIN LGDAT.AVTX ON
		AVTCO# = IGCOM# AND
		AVTCHQ	= IGCHQ#
	LEFT OUTER JOIN LGDAT.VCHR ON
		IDCOM# = IGCOM# AND
		IDVCH# = AVTVH#
WHERE
	DKFSYY = 13 AND
	DKFSPR = 8 AND
	DKSRCE = 'AP' AND
	DKQUAL = 'CQ' AND
	DKKEYN = '000006762'
		