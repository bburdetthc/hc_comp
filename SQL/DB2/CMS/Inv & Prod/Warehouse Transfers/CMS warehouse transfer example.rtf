SELECT
	DKFSYR,DKFSYY,DKFSPR,DKSRCE,DKQUAL,DKKEYN,DKACC#, DKAMT,DKREF#,DKREFD,DKADDD,DKPDAT,DKTDAT,DKBTC#,DKFUT4,DKPART,DKFUT9
FROM
	LGDAT.GLSBIV
WHERE
	DKSRCE = 'IC'AND
	DKQUAL = '' AND
	DKREFD = '12.00 VL DISH BK' AND
	DKFSYR = '20' AND
	DKFSYY = '13' AND
	DKFSPR = '03' AND
	DKBTC# = 196258
ORDER BY DKACC#