SELECT 
	PJT0.ACCTID, PJT0.FSCSYEAR, PJT0.JRNLPERD, PJT0.JRNLID, PJT0.JEDESC, PJT0.JEAMT, PJT0.JE1CMNT, PJT0.JE2CMNT, PJT0.JE3CMNT, PJT0.JE4CMNT, PJT0.REF, PJT0.EXTLKEY, PJT0.REFUSER
FROM 
	S107B59D.M20DINST.PJT0 PJT0
WHERE 
	substr(PJT0.ACCTID,1,4) IN ('1093') AND
	SUBSTR(ACCTID,8,4) = '7250' AND
	PJT0.FSCSYEAR='2012'