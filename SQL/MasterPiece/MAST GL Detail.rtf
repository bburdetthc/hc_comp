SELECT 	
	SUBSTR(PJT0.ACCTID,3,2) AS COMP,
	SUBSTR(PJT0.ACCTID,8,4) AS OBJ,
	PJT0.ACCTID, 
	SUBSTR(FSCSCURN,1,2) AS CURR,
	'' AS HYPACCT,
	SUBSTR(PJT0.FSCSYEAR,3,2)||PJT0.JRNLPERD AS PERD,
	PJT0.JEAMT,
	PJT0.JRNLID, 
	PJT0.JEDESC
FROM 	
	S107B59D.M20DINST.PJT0 PJT0
WHERE 	
	SUBSTR(ACCTID,1,4) In ('1093','1065','1088','1064','1057','1059','1058') AND 
	SUBSTR(ACCTID,6,1) In ('6') AND 
	PJT0.FSCSYEAR='2013' AND
	PJT0.JRNLPERD <= '06'
FETCH FIRST 10 ROWS ONLY